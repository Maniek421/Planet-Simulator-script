--Hub
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Hub                                                                    Version 0.3.2", "Serpent")
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Loaders")
--Scripts
--Planet Simulator
MainSection:NewButton ("Planet Simulator", "Click to load script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Maniek421/Scripts/main/Planet%20Simulator"))()
end)
--Roblox but every second you get +1 Jump
MainSection:NewButton ("Roblox but every second you get +1 Jump", "Click to load script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Maniek421/Scripts/main/Planet%20Simulator"))()
end)
--Roblox But Every Second You Get +1 Jump
MainSection:NewButton ("Roblox But Every Second You Get +1 Jump", "Click to load script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Maniek421/Scripts/main/Roblox%20but%20every%20second%20you%20get%20%2B1%20Jump"))()
end)
