MiscSection:NewSlider("Speed Power", "Speed!!", 250, 16, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

MiscSection:NewSlider("Jump Power", "Jump!!", 250, 20, function(s)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

MiscSection:NewButton("Gamepasses", "Gives you all gamepasses in game", function()
    local pass = require(game:GetService("ReplicatedStorage").Framework.Client["5 | Gamepasses"])

    pass.Owns = function() return true end
end)


